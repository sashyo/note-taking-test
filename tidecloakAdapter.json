{
  "realm": "secure-notes",
  "auth-server-url": "https://your-tidecloak-server.com/auth",
  "ssl-required": "external",
  "resource": "secure-notes-client",
  "public-client": true,
  "confidential-port": 0,
  "enable-cors": true,
  "cors-max-age": 1000,
  "cors-allowed-methods": "POST, PUT, DELETE, GET",
  "cors-allowed-headers": "X-Requested-With, Content-Type, Authorization, Origin, Accept, Access-Control-Request-Method, Access-Control-Request-Headers",
  "bearer-only": false,
  "autodetect-bearer-only": false,
  "connection-pool-size": 20,
  "socket-timeout-millis": 60000,
  "connection-timeout-millis": 60000,
  "connection-ttl-millis": 60000,
  "disable-trust-manager": false,
  "allow-any-hostname": false,
  "truststore": "path/to/truststore.jks",
  "truststore-password": "password",
  "client-keystore": "path/to/client-keystore.jks",
  "client-keystore-password": "password",
  "client-key-password": "password",
  "token-minimum-time-to-live": 0,
  "min-time-between-jwks-requests": 10,
  "public-key-cache-ttl": 86400,
  "redirect-rewrite-rules": {}
}